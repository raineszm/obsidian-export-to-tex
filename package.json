{
  "name": "obsidian-export-to-tex",
  "version": "0.2.2",
  "description": "Export vault files in a format amenable to pasting into a tex document",
  "main": "src/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && cp manifest.json dist/",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "release": "standard-version",
    "release:dryrun": "standard-version --dry-run",
    "release:prerelease": "standard-version --skip.tag --skip.changelog --prerelease",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.json": "prettier --write"
  },
  "standard-version": {
    "tag-prefix": "",
    "bumpFiles": [
      "package.json",
      "manifest.json",
      {
        "filename": "versions.json",
        "updater": ".standard-version/standard-version-versions-updater.js"
      }
    ]
  },
  "keywords": [],
  "author": "Zach Raines",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:raineszm/obsidian-export-to-tex.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "@types/codemirror": "^0.0.98",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.12",
    "@types/normalize-path": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "commitizen": "^4.2.4",
    "electron": "^11.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "obsidian": "github:obsidianmd/obsidian-api#ea530cce1adaa33eb891f053a56a12fa8640b834",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "ts-jest": "^26.5.6",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vfile": "^4.2.1"
  },
  "dependencies": {
    "@types/github-slugger": "^1.3.0",
    "github-slugger": "^1.4.0",
    "normalize-path": "^3.0.0",
    "rebber": "^5.2.1",
    "remark-directive": "^1.0.1",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "remark-parse": "^9.0.0",
    "remark-wiki-link": "^1.0.4",
    "unified": "^9.2.2",
    "unist-util-filter": "^2.0.3",
    "unist-util-is": "^4.1.0",
    "unist-util-visit": "^2.0.3",
    "vfile-reporter": "^6.0.2"
  }
}
