{
  "name": "obsidian-export-to-tex",
  "version": "0.0.1",
  "description": "Export vault files in a format amenable to pasting into a tex document",
  "main": "src/main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "commit": "git-cz",
    "release": "standard-version",
    "release:dryrun": "standard-version --dry-run",
    "release:prerelease": "standard-version --skip.tag --skip.changelog --prerelease"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.json": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "tag-prefix": "",
    "bumpFiles": [
      "package.json",
      "manifest.json",
      {
        "filename": "versions.json",
        "updater": ".standard-version/standard-version-versions-updater.js"
      }
    ]
  },
  "keywords": [],
  "author": "Zach Raines",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:raineszm/obsidian-export-to-tex.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/codemirror": "^0.0.98",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "commitizen": "^4.2.2",
    "electron": "^11.0.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "obsidian": "obsidianmd/obsidian-api#6647842a61850b716eb05af0f9ffe36067dabbef",
    "prettier": "^2.2.0",
    "rollup": "^2.33.3",
    "rollup-plugin-copy": "^3.3.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "vfile": "^4.2.1"
  },
  "dependencies": {
    "@types/github-slugger": "^1.3.0",
    "github-slugger": "^1.3.0",
    "loglevel": "^1.7.1",
    "rebber": "^5.1.2",
    "remark-directive": "^1.0.1",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "remark-parse": "^9.0.0",
    "remark-wiki-link": "^1.0.0",
    "unified": "^9.2.0",
    "unist-util-filter": "^2.0.3",
    "unist-util-is": "^4.0.4",
    "unist-util-visit": "^2.0.3"
  }
}
